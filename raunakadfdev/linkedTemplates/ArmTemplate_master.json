{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "raunakadfdev"
		},
		"AzureSqlDatabase_Source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_Source'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=raunakairplanedev.database.windows.net;Initial Catalog=airplanedevdb;User ID=raunak_admin"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raunakdatalakesource.dfs.core.windows.net"
		},
		"AzureDataLakeStorage1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "fd2e37d1-f253-4668-bef8-405466d1b986"
		},
		"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "47ebd300-1259-4a10-a976-674fa4927141"
		},
		"AzureDataLakeStorageDev_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airlinedatalakedev.dfs.core.windows.net/"
		},
		"AzureDataLakeStorageDev_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "fd2e37d1-f253-4668-bef8-405466d1b986"
		},
		"AzureDataLakeStorageDev_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "47ebd300-1259-4a10-a976-674fa4927141"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://airlinekeyvaultdev.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "raunakadfdev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_Source_connectionString": {
						"value": "[parameters('AzureSqlDatabase_Source_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeStorageDev_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageDev_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorageDev_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorageDev_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorageDev_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorageDev_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}